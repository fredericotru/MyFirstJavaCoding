import java.util.Scanner;

class Main {
  public static void main(String[] args) {
  Scanner n1 = new Scanner(System.in);
  double cel;
  double faren;
    System.out.println("Digite a temperatura em C ");
    cel = n1.nextDouble();
  faren = (cel*1.8)+32;
  System.out.printf("Farenheit = %.2f", faren);
  n1.close();
  }
}
